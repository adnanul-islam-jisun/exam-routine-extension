{
  "manifest_version": 3,
  "name": "Student ID Extractor",
  "version": "1.0",
  "description": "Extracts student ID from webpage",
  "permissions": ["storage", "tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
